<div class="row">
  <div class="col-4-4 place-pane">
    <input type="hidden" name="id" value="{{id}}">
    <div class="row">
      <div class="col-4-4 place-name">
        <h3>
          <i class="fa fa-home"></i> {{name}}
        </h3>
      </div>
    </div>
    <div class="row">
      <div class="col-4-4 place-content">
        <div class="row">
          <div class="col-4-4 place-set-content">
            <div class="row">
              <div class="col-4-4 place-cars">
                {{#each cars}}
                  {{> car this}}
                {{/each}}
              </div>
            </div>
          </div>
        </div>
        {{#if persons.length}}
          <div class="row">
            <div class="col-4-4 place-unset-content">
              <div class="row">
                <div class="col-4-4">
                  Things not in place
                </div>
              </div>
              <div class="row">
                <div class="col-4-4">
                  {{#each persons}}
                    {{> person this}}
                  {{/each}}
                </div>
              </div>
            </div>
          </div>
        {{/if}}
      </div>
    </idv>
  </div>
</div>